
<h1>Edit post</h1>

<%= form_for @post do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title %> <br><br>

  <%= f.label :content %>:
  <%= f.text_field :content %> <br><br>

  <%= f.label :user %>: <br>
  <%= f.collection_select :user_id, User.all, :id, :username %>

  <br><br>

  <%= f.submit %>
<% end %>
